    "project": "Gson",
    "docker_image": "maven:3-jdk-8-slim",
    "build": "mvn -f gson/pom.xml clean compile",
    "build_test": "mvn -f gson/pom.xml test-compile",
    "test_command": "mvn -f gson/pom.xml -Dtest=%s test",
    "test_path": "gson/src/test/java/%s.java",
    "report_path": "gson/target/surefire-reports/TEST-%s.xml",
